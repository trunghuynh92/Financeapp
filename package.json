{
  "name": "financeapp-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@financeapp/web",
    "dev:web": "npm run dev --workspace=@financeapp/web",
    "build": "npm run build --workspace=@financeapp/web",
    "build:web": "npm run build --workspace=@financeapp/web",
    "start": "npm run start --workspace=@financeapp/web",
    "lint": "npm run lint --workspace=@financeapp/web"
  },
  "devDependencies": {
    "typescript": "^5"
  }
}
